<IfModule mod_deflate.c>
  AddOutputFilterByType DEFLATE text/text text/html text/plain text/xml text/css application/x-javascript application/javascript image/svg+xml
</IfModule>

<IfModule mod_headers.c>
  Header set Connection keep-alive

  <filesMatch "\.(ico|pdf|flv|jpg|jpeg|png|gif|js|swf|woff|woff2|svg)$">
    Header set Cache-Control "max-age=31536000, public"
  </filesMatch>

  <filesMatch "\.(css)$">
    Header set Cache-Control "max-age=31536000, public, must-revalidate"
    #Header set Cache-Control "max-age=0, public, must-revalidate"
  </filesMatch>

  <filesMatch "\.(xml|txt)$">
    Header set Cache-Control "max-age=172800, public, must-revalidate"
  </filesMatch>

  <filesMatch "\.(html|htm)$">
    Header set Cache-Control "max-age=7200, must-revalidate"
  </filesMatch>

  <FilesMatch "\.(gif|jpg|png|ico|css|js|pdf|txt)$">
    Header append Cache-Control "public"
  </FilesMatch>
</IfModule>